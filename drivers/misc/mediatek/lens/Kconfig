# LENS
#

config MTK_LENS_CACTUS_DW9714AF_OFILM_SUPPORT
	bool "CACTUS DW9714AF OFILM Lens Driver"
	depends on MTK_LENS_MAIN
	default n
	help
	  CACTUS DW9714AF OFILM Lens Driver
	  This config is used to enable the corresponding
	  lens driver for the camera sensor module
	  Set as y if the driver is used in this project

