config MTK_LENS
	bool CONFIG_MTK_LENS
	default n
	help
	  CONFIG_MTK_LENS

config MTK_LENS_MAIN
	bool CONFIG_MTK_LENS_MAIN
	depends on MTK_LENS
	default n
	help
	  CONFIG_MTK_LENS_MAIN

config MTK_LENS_MAIN2
	bool CONFIG_MTK_LENS_MAIN2
	depends on MTK_LENS
	default n
	help
	  CONFIG_MTK_LENS_MAIN2

